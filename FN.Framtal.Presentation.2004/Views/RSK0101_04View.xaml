                </Grid>


            </Border>

            <Border Grid.Row="2" Grid.Column="0" Grid.RowSpan="6" Grid.ColumnSpan="5" BorderThickness="1 0 1 0" Background="{StaticResource DefaultBrush}">
                <DataGrid />
            </Border>

        </Grid>


        <!--#region Bottom and right side -->
        <TextBlock Grid.Column="1" Grid.Row="22" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0 0 0 0" FontSize="{StaticResource Chapter2BigTextFontSize}" FontWeight="Bold">Bls. 4</TextBlock>
        <Rectangle Grid.Column="5" Grid.RowSpan="25" Fill="{StaticResource MediumBrush}"></Rectangle>
        <!--#endregion-->

    </Grid>
