                    </Border>
                </Grid>


            </Border>

            <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="4" Grid.ColumnSpan="5" BorderThickness="1 0 1 0" Background="{StaticResource DefaultBrush}">
                <DataGrid />
            </Border>

        </Grid>

        <!--#endregion-->


        <!--#region 4.2 -->
