                </Border>
            </Grid>

            <Border Grid.Row="1" Grid.Column="2" Padding="3" Background="{StaticResource LightBrush}">
                <controls:NumericTextBox  BorderThickness="0" />
            </Border>

            <Border Grid.Row="1" Grid.Column="3" Background="{StaticResource LightBrush}" BorderThickness="0" Margin="5,0,0,0" >
                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Left">
                    <Border Background="{DynamicResource LightBrush}" Width="25" BorderThickness="1">
                        <TextBlock  VerticalAlignment="Center" HorizontalAlignment="Center" Text="037" />
                    </Border>
                    <Border Padding="3" Width="155" Background="{StaticResource LightBrush}">
                        <controls:NumericTextBox  BorderThickness="0" />
                    </Border>
                </StackPanel>
            </Border>

        </Grid>

    </Grid>
</UserControl>
